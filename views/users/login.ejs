<form class="user-form container h-100" action="/users/goLogin" method="post">
  <h1>login</h1>
  <div class="form-group row">
    <label for="USER_ID" class="col-sm-3 col-form-label">ID</label>
    <div class="col-sm-9">
      <input type="text" id="USER_ID" name="USER_ID" value="" class="form-control">
    </div>
  </div>


  <div class="form-group row">
    <label for="USER_PW" class="col-sm-3 col-form-label">PW</label>
    <div class="col-sm-9">
      <input type="password" id="USER_PW" name="USER_PW" value="" class="form-control">
    </div>
  </div>

  <!-- Submit button -->
  <div class="form-group row">
    <button type="submit" class="btn btn-primary btn-block mb-4">
      Login
    </button>
  </div>

  <!-- Register buttons -->
  <div class="text-center form-group row">
    <p>Not a member? <a href="/users/sign">Register</a></p>
  </div>

</form>